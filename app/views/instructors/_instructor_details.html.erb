<h3><%= @instructor.proper_name %></h3>

<h5>Basic Information</h5>
<% unless @instructor.photo_url.nil? %>
    <p><%= image_tag @instructor.photo_url %></p>
<% end %>
<p><strong>Bio: </strong><%= @instructor.bio %></p>
<p><strong>Email: </strong><%= @instructor.email %></p>
<p><strong>Phone: </strong><%= number_to_phone @instructor.phone %></p>
<p><strong>Username: </strong><%= @instructor.username %> </p>
<p><strong>Active:</strong>
  <%= @instructor.active ? "Yes" : "No" %>
</p>