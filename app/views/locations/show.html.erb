<div class="body-content">
  <div class="row">
    <%= render :partial => "partials/back_button", locals: {model_name: "location", object: @location} %>
  </div>
  <div class="row">
    <div class="col s4">
      <%= render :partial => "location_details" %>
    </div>
    <div class="col s8">
      <%= render :partial => "partials/camps_list", locals: {camps: @upcoming_camps_at_location, state: "Upcoming"} %>
    </div>
  </div>
  <div class="row">
    <div class="col s12">
      <%= render :partial => "partials/object_options", locals: {model_name: "location", object: @location} %>
    </div>
  </div>
</div>
<%# link_to 'Edit', edit_location_path(@location) %>
<%# link_to 'Back', locations_path %>