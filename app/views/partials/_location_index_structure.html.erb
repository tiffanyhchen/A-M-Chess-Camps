<div class="row">
  <div class="col s4">
    <h3><%= model_name.capitalize %></h3>
    <!-- Add a button for new instances of object -->
    <p>Find our various camp locations.</p>
    <% if logged_in? && ( current_user.role?(:admin) ) %>
      <%= link_to(eval("new_#{model_name}_path")) do %>
        <!-- <i class="fa fa-plus"></i> -->
        &nbsp;+ NEW <%= model_name.upcase %>
      <% end %>
    <% end %>
  </div>

  <div class="col s8">
    <ul id="tabs-swipe-demo" class="tabs active">
      <li class="tab col s4"><a href="#test-swipe-1">Active Locations</a></li>
      <li class="tab col s4"><a href="#test-swipe-2">Inactive Locations</a></li>
    </ul>
    <div id="test-swipe-1" class="col s12"><%= render partial: "#{primary}" %></div>
    <div id="test-swipe-2" class="col s12">
      <% if logged_in? && ( current_user.role?(:admin) ) %>
        <% unless secondary.nil? %>
          <p>&nbsp;</p>
          <%= render partial: "#{secondary}" %>
        <% end %>
      <% end %>
    </div>

  </div>

</div>

