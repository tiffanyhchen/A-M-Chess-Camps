<div class="card">
  <div class="card-content">
    <span class="card-title">Other Students</span>
    <% if @other_registrations.empty? %>
      <p>No other students signed up at this time</p>
    <% else %>
      <ul>
        <% for s in @other_registrations %>
          <li><%= link_to s.proper_name, student_path(s) %></li>
        <% end %>
      </ul>
    <% end %>
  </div>
</div>