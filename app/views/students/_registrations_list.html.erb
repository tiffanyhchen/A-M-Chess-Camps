<div class="card">
    <div class="card-content">
      <span class="card-title">Registered Camps</span>
      <table class="highlight">
        <thead>
          <th>Camp name</th>
          <th>Start Date</th>
        </thead>

        <% for r in @registrations %>
          <tr>
            <td><%= link_to r.camp.name, camp_path(r.camp) %></td>
            <td><%= r.camp.start_date.strftime("%m/%d/%y") %></td>
          </tr>
        <% end %>
      </table>
      <br />
    </div>
</div>