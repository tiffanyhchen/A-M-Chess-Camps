<h4>Registered Camps</h4>

<table class="striped highlight">
  <thead>
    <th>Camp name</th>
    <th>Dates </th>
  </thead>

  <% for r in @registrations %>
    <tr>
      <td><%= link_to r.camp.name, camp_path(r.camp) %></td>
      <td><%= r.camp.start_date.strftime("%m/%d/%y") %> - <%= r.camp.end_date.strftime("%m/%d/%y") %></td>
    </tr>
  <% end %>
</table>
<br />